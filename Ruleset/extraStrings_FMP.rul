      STR_WASPITE_CORPSE: "Waspite Corpse"
      STR_WASPITE_RED_CORPSE: "Waspite Corpse"
      STR_WASPITE_BROWN_CORPSE: "Waspite Corpse"
      STR_WASPITE_WHITE_CORPSE: "Waspite Corpse"
      STR_WASPITE_PURPLE_CORPSE: "Waspite Corpse"

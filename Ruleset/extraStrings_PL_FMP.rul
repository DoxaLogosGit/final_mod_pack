      STR_REPAIR_JUGGERNAUT_SUIT: "Naprawa Dominatora"
      STR_JUGGERNAUT_SUIT_UC: "DOMINATOR"
      STR_JUGGERNAUT_SUIT_UFOPEDIA: "Ten superciężki pancerz został zaprojektowany dla jak najlepszej ochrony przed wysokoenergetycznymi brońmi obcych."
      STR_CORPSE_JUMP_ARMOR: "Uszkodzony pancerz desantowy"
      STR_CORPSE_ARMOR: "Uszkodzony pancerz osobisty"
      STR_REPAIR_JUMP_ARMOR: "Naprawa pancerza desantowego"
      STR_REPAIR_PERSONAL_ARMOR: "Naprawa pancerza osobistego"

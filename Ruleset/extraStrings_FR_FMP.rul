extraStrings:
  - type: fr
    strings:
      STR_MEDAL_MERIT_STAR_NAME: "Medal of Aggression"
      STR_MEDAL_MERIT_STAR_DESCRIPTION: "Remise à un soldat ayant éliminés de nombreux extra-terrestres."
      STR_MEDAL_XENOCIDE_MEDAL_NAME: "{0} Xenocide Medal"
      STR_MEDAL_XENOCIDE_MEDAL_DESCRIPTION: "Remise à un soldat ayant combattu la menace {0} avec succès."
      STR_MEDAL_DEFENDER_MEDAL_NAME: "Guardian Star"
      STR_MEDAL_DEFENDER_MEDAL_DESCRIPTION: "Remise à un soldat ayant pris part à une mission de défense de base victorieuse."
      STR_MEDAL_MILITARY_CROSS_NAME: "Military Cross"
      STR_MEDAL_MILITARY_CROSS_DESCRIPTION: "Remise à un soldat ayant participé à un grand nombre de missions."
      STR_MEDAL_SERVICE_MEDAL_NAME: "Long Service Medal"
      STR_MEDAL_SERVICE_MEDAL_DESCRIPTION: "Remise à un soldat en service pour une longue période."
      STR_MEDAL_HONOR_CROSS_NAME: "XCOM Service Medal"
      STR_MEDAL_HONOR_CROSS_DESCRIPTION: "Remise à tout nouveau soldat en reconnaissance de sa dévotion dans le combat contre la menace extra-trrester et de prendre part à l'effort de guerre pour sauver l'humanité. Cette médaille n'a pas de décoration supplémentaire."
      STR_MEDAL_CAMPAIGN_MEDAL_NAME: "Order of the Custodian"
      STR_MEDAL_CAMPAIGN_MEDAL_DESCRIPTION: "Remise à un soldat ayant défendu beaucoup de régions avec succès."
      STR_MEDAL_SPECIALISTS_MEDAL_NAME: "Police Medal"
      STR_MEDAL_SPECIALISTS_MEDAL_DESCRIPTION: "Remise à un soldat ayant réussi à contrôler de nombreux aliens sans avoir eu besoin de recourir à la force."
      STR_MEDAL_WEAPON_PROFICIENCY_NAME: "{0} Proficiency Medal"
      STR_MEDAL_WEAPON_PROFICIENCY_DESCRIPTION: "Remise à un soldat s'étant distingué dans l'usage de {0}."
      STR_MEDAL_CAMPAIGN_RIBBON_NAME: "{0} Campaign Ribbon"
      STR_MEDAL_CAMPAIGN_RIBBON_DESCRIPTION: "Remise à un soldat ayant été actif dans la protection de {0}."
      STR_MEDAL_ORDER_OF_RESTRAINT_NAME: "Order of Restraint"
      STR_MEDAL_ORDER_OF_RESTRAINT_DESCRIPTION: "Remise à un soldat ayant neutralisé beaucoup d'extra-terrestres sans faire usage de la force."
      STR_MEDAL_LONGEST_NIGHT_NAME: "Longest Night Ribbon"
      STR_MEDAL_LONGEST_NIGHT_DESCRIPTION: "Remise à un soldat ayant assisté aux horreurs des missions nocturnes de terreurs aliennes."
      STR_MEDAL_MEDAL_OF_HEROISM_NAME: "Medal of Heroism"
      STR_MEDAL_MEDAL_OF_HEROISM_DESCRIPTION: "Remise à un soldat s'étant battu jusqu'à épuisement."
      STR_MEDAL_HELLRAISER_MEDAL_NAME: "Inferno Star"
      STR_MEDAL_HELLRAISER_MEDAL_DESCRIPTION: "Remise à un soldat ayant utilisé des armes incdendiaires afin d'éliliner des extra-terrestres."
      STR_MEDAL_DISTINGUISHED_SERVICE_NAME: "Order of the Hammer"
      STR_MEDAL_DISTINGUISHED_SERVICE_DESCRIPTION: "Remise à un soldat ayant tués des soldats extra-terrestres de tous types."
      STR_MEDAL_BOLTS_CROSS_NAME: "Bolt's Cross"
      STR_MEDAL_BOLTS_CROSS_DESCRIPTION: "Remise à un soldat n'ayant pas fait appel à la force pour neutraliser des cibles importantes."
      STR_MEDAL_STAR_OF_ASCLEPIUS_NAME: "Star of Asclepius"
      STR_MEDAL_STAR_OF_ASCLEPIUS_DESCRIPTION: "Remise à un soldat ayant subit le feu nourri de la part de l'ennemi."
      STR_MEDAL_MAN_OF_STEEL_NAME: "Man of Steel"
      STR_MEDAL_MAN_OF_STEEL_DESCRIPTION: "Remise à un soldat ayant été touchés plusieurs fois lors d'une seule mission."
      STR_MEDAL_BLACK_CROSS_NAME: "Black Cross"
      STR_MEDAL_BLACK_CROSS_DESCRIPTION: "Remise à un soldat ayant survécu à un tir fraternel."
      STR_MEDAL_STAR_OF_VALOR_NAME: "Star of Valor"
      STR_MEDAL_STAR_OF_VALOR_DESCRIPTION: "Remise à un soldat ayant survécu seul à une mission."
      STR_MEDAL_IRON_MAN_NAME: "Iron Man"
      STR_MEDAL_IRON_MAN_DESCRIPTION: "Remise à un soldat qui est a montré clairement ses capacités d'Iron Man ou Iron Woman."
      STR_MEDAL_ORDER_OF_EARTH_NAME: "Order of Earth"
      STR_MEDAL_ORDER_OF_EARTH_DESCRIPTION: "Remise à un soldat ayant été actif en défendant la Terre."
      STR_MEDAL_SNIPER_TMPNAME: "Marksman Citation"
      STR_MEDAL_SNIPER_TMPDESCRIPTION: "Remise à un sodat ayant réussi d'éliminer des ennemis qu'il visait depuis une grande distance ."
      STR_MEDAL_LUCKYSHOT_TMPNAME: "Good Luck Citation"
      STR_MEDAL_LUCKYSHOT_TMPDESCRIPTION: "Remise à un soldat ayant toucher sa cible en réussissant un tir improbable défiant les lois de la nature"
      STR_MEDAL_DBLKILL_TMPNAME: "Performance Citation"
      STR_MEDAL_DBLKILL_TMPDESCRIPTION: "Remise à un soldat ayant démontré sa vaillance sur le champs de bataille en éliminant plusieurs cibles ennemis en un cours laps de temps."
      STR_MEDAL_REACTIONFIRE_TMPNAME: "Swiftness Citation"
      STR_MEDAL_REACTIONFIRE_TMPDESCRIPTION: "Remise à un soldat ayant démontré ses réflexes en éliminant un ennemi avant que celui-ci n'ait eu le temps de réagir."
      STR_MEDAL_PURPLE_HEART_NAME: "Purple Heart"
      STR_MEDAL_PURPLE_HEART_DESCRIPTION: "Remise à un soldat ayant été blessé au champs d'honneur."
      STR_MEDAL_CRIMSON_HEART_NAME: "Crimson Heart"
      STR_MEDAL_CRIMSON_HEART_DESCRIPTION: "Remise à un soldat ayant été blessé durant une longue période."
      STR_MEDAL_ORDER_OF_GLORY_NAME: "Order of Glory"
      STR_MEDAL_ORDER_OF_GLORY_DESCRIPTION: "Remise à un soldat ayant été décisif au succès d'une mission."
      STR_MEDAL_VALIANT_CRUX_NAME: "Valiant Crux"
      STR_MEDAL_VALIANT_CRUX_DESCRIPTION: "Remise à un soldat ayant protégé tous les civils lors d'une mission de terreur alienne."
      STR_MEDAL_FALLEN_NAME: "Order of the Fallen"
      STR_MEDAL_FALLEN_DESCRIPTION: "Remise à un soldat tombé au combat en honneur du sacrifice consenti et de son dévouement dans la défense de la Terre."

      STR_XCOM_OPERATIVES_RETIRED_THROUGH_INJURY: "ŻOŁNIERZY X-COM-u ZWOLNIONYCH Z POWODU OBRAŻEŃ"
      STR_CIVILIANS_KILLED_BY_XCOM_OPERATIVES: "CYWILI ZABITYCH PRZEZ ŻOŁNIERZY X-COM-u"
      STR_CIVILIANS_KILLED_BY_ALIENS: "CYWILI ZABITYCH PRZEZ OBCYCH"
      STR_RESEARCH_COMPLETED: "Badania ukończone"
      STR_GAME_OVER_2: "Wiedza zdobyta podczas projektu XCom została utracona na zawsze. Nie udało ci się obronić Ziemi."
      STR_LABORATORY_SPACE_AVAILABLE: "Wolnych stanowisk badawczych>{ALT}{0}"
